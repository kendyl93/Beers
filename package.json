{
  "name": "Beers",
  "version": "1.0.0",
  "description": "App created for an interview process",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --mode development",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "format": "prettier --check \"{client,server}/**/*.{js,jsx}\"",
    "format:fix": "prettier --write \"{client,server}/**/*.{js,jsx}\"",
    "lint": "eslint ./ --ext \".js,.jsx\" --max-warnings=0",
    "lint:fix": "eslint ./ --ext \".js,.jsx\" --max-warnings=0 --fix",
    "stylelint": "stylelint client/**/*.scss --cache",
    "stylelint:fix": "stylelint client/src/**/*.scss --cache --fix"
  },
  "lint-staged": {
    "**/{**/!(*.min).js,*.jsx,*.json,*.scss,*.css,*.md}": [
      "prettier --write",
      "git add"
    ],
    "**/{**/!(*.min).js,*.jsx}": [
      "eslint -c .eslintrc --fix",
      "echo test",
      "git add"
    ],
    "**/*.scss": [
      "stylelint --syntax scss",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged &&  npm run test",
      "pre-push": "lint-staged &&  npm run test"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/src/tests/__mocks__/styleMock.js",
      "\\.(png|jpg|gif|ttf|eot|svg)$": "<rootDir>/src/tests/__mocks__/fileMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/setupTest.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kendyl93/Beers.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kendyl93/Beers/issues"
  },
  "homepage": "https://github.com/kendyl93/Beers#readme",
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/polyfill": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/preset-react": "7.8.3",
    "babel-eslint": "10.0.3",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "css-loader": "3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.8.0",
    "eslint-import-resolver-webpack": "0.12.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "file-loader": "^5.0.2",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.2.0",
    "jest": "25.1.0",
    "lint-staged": "10.0.2",
    "node-sass": "4.13.1",
    "prettier": "1.19.1",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "stylelint": "^13.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-scss": "^3.13.0",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0"
  }
}
